{
	"name": "@kirick/ip",
	"version": "0.1.1-beta.2",
	"description": "IP address library",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "src/main.js",
	"exports": {
		".": {
			"import": "./src/main.js",
			"require": "./dist/ip.cjs"
		}
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"dependencies": {
		"ip-address": "8.1.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.21.8",
		"eslint": "8.41.0",
		"eslint-config-xo": "0.43.1",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jsdoc": "46.5.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "47.0.0",
		"jest": "^29.7.0"
	},
	"scripts": {
		"test": "bun test && npm run test-node",
		"test-node": "NODE_OPTIONS=--experimental-vm-modules jest --forceExit",
		"coverage": "bun test --coverage",
		"build": "bun run build-cjs",
		"build-cjs": "bunx esbuild --bundle --define:IS_BUILD=true --pure:createRequire --tree-shaking=true --platform=node --format=cjs --packages=external --outfile=dist/ip.cjs src/main.js",
		"npm-publish": "bun run build && bun run eslint . && bun run test && npm publish --tag beta"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kirick13/ip-js.git"
	},
	"keywords": [
		"ip",
		"address"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kirick13/ip-js/issues"
	},
	"homepage": "https://github.com/kirick13/ip-js#readme"
}
