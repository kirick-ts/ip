{
	"name": "@kirick/ip",
	"version": "0.1.6",
	"description": "IP address library",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/main.js",
	"exports": {
		".": {
			"types": {
				"import": "./dist/main.d.ts",
				"require": "./dist/main.d.cts"
			},
			"import": "./dist/main.js",
			"require": "./dist/main.cjs"
		}
	},
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"ip-address": "10.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@kirick/lint": "0.2.6",
		"@types/node": "22.16",
		"eslint": "9.27.0",
		"oxlint": "1.3.0",
		"publint": "0.3.12",
		"tsdown": "0.12.9",
		"typescript": "5.8.3",
		"unplugin-unused": "0.5.1",
		"valibot": "1.1.0",
		"vitest": "3.2.4"
	},
	"scripts": {
		"build": "tsdown src/main.ts --publint --unused --dts --format esm --format cjs",
		"check": "bun run lint && bun run build && bun run test",
		"lint": "biome format && oxlint && eslint . && tsc",
		"test": "npm run test:vitest && bun test --coverage",
		"test:vitest": "vitest run --no-file-parallelism"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kirick-ts/ip.git"
	},
	"keywords": [
		"ip",
		"address"
	],
	"author": "Daniil Kirichenko (https://twitter.com/kirickme)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kirick13-ts/ip/issues"
	},
	"homepage": "https://github.com/kirick13-ts/ip#readme"
}
